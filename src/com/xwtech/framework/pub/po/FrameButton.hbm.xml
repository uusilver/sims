<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class 
    name="com.xwtech.framework.pub.po.FrameButton" 
    table="FRAME_BUTTON"
>

    <id
        name="buttonId"
        type="java.lang.Long"
        column="BUTTON_ID"
    >
        <generator class="assigned" />
    </id>

    <property
        name="buttonName"
        type="java.lang.String"
        column="BUTTON_NAME"
        not-null="true"
        length="50"
    />
    <property
        name="buttonAttr"
        type="java.lang.String"
        column="BUTTON_ATTR"
        length="500"
    />
    <property
        name="buttonRegexp"
        type="java.lang.String"
        column="BUTTON_REGEXP"
        length="500"
    />
    <property
        name="buttonState"
        type="java.lang.String"
        column="BUTTON_STATE"
        not-null="true"
        length="1"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to FrameButtonUrl -->
    <set
        name="frameButtonUrls"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="BUTTON_ID" />
        </key>
        <one-to-many 
            class="com.xwtech.framework.pub.po.FrameButtonUrl"
        />
    </set>

</class>
</hibernate-mapping>
